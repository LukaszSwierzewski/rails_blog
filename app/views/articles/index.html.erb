<h1>All Articles</h1>
<button>
<%= link_to  'create new article', new_article_path,
class: 'article_button'
  %>
</button>
<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article),
      class: 'article_title', id: "article_#{article.id}"%>
    </li>
    <h5> <%= article.body %></h5>
  <% end %>
</ul>
